<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Or>
            <Class name="~.*\.AllTests"/>
            <Class name="~.*TestCase"/>
            <Class name="~.*BeanInfo"/>
        </Or>
    </Match>
    <Match>
        <Class name="deors.core.sensible.SensibleDataType"/>
        <Method name="&lt;init&gt;"/>
        <Bug pattern="DM_STRING_VOID_CTOR"/>
    </Match>
    <Match>
        <Class name="deors.core.sensible.SensibleDataType"/>
        <Method name="clone"/>
        <Bug pattern="DM_STRING_CTOR"/>
    </Match>
    <Match>
        <Class name="deors.core.sensible.SensibleObject"/>
        <Or>
            <Method name="changeField" params="int,java.lang.String" returns="void"/>
            <Method name="changeField" params="int,deors.core.sensible.SensibleDataType" returns="void"/>
            <Method name="setField" params="int,java.lang.String" returns="void"/>
            <Method name="setField" params="int,deors.core.sensible.SensibleDataType" returns="void"/>
        </Or>
        <Bug pattern="DM_STRING_CTOR"/>
    </Match>
    <Match>
        <Class name="deors.core.sensible.SensibleString"/>
        <Method name="&lt;init&gt;"/>
        <Bug pattern="DM_STRING_VOID_CTOR"/>
    </Match>
</FindBugsFilter>
